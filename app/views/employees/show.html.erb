<h3><%= @employee.last_name_first_name %></h3>
<p>Alias: <%= @employee.alias %></p>
<p>Title: <%= @employee.title %></p>
<p>Office: <%= @employee.office %></p>
<img src=<%= @employee.img_url %> alt=<%= @employee.first_name %>>
<%= button_to "Edit Employee Info", edit_employee_path(@employee), method: :get %> 
<%= button_to "Delete this Employee", employee_path(@employee), method: :delete %>